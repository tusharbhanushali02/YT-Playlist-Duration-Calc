{
   "action": {
      "default_icon": {
         "16": "/icons/icon.png"
      },
      "default_title": "YTPD Calc By Tushar Bhanushali"
   },
   "content_scripts": [ {
      "js": [ "src/library.js", "src/content.js" ],
      "matches": [ "https://www.youtube.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "An extension to calculate & display the total duration of a youtube playlist by Tushar Bhanushali.",
   "host_permissions": [ "https://www.youtube.com/*" ],
   "icons": {
      "128": "/icons/icon.png",
      "16": "/icons/icon.png",
      "48": "/icons/icon.png"
   },
   "manifest_version": 3,
   "name": "Youtube Playlist Duration Calculator By Tushar Bhanushali",
   "short_name": "YTPD Calc By Tushar Bhanushali",
   "version": "1"
}
